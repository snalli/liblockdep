   (__extension__ ({ size_t __cpu = (47); __cpu < 8 * (sizeof (cpu_set_t)) ? (((__cpu_mask *) ((&mask)->__bits))[((__cpu) / (8 * sizeof (__cpu_mask)))] |= ((__cpu_mask) 1 << ((__cpu) % (8 * sizeof (__cpu_mask))))) : 0; }));
  }
  rc = sched_setaffinity(tid, sizeof(cpu_set_t), &mask);
  if (rc < 0)
   perror("sched_setaffinity"), exit(1);
 } else if (mode == 'c') {
  if (gid == 0)
   set_group(tid, "a");
  else
   set_group(tid, "b");
 }

 atomic_inc(&ready_count);

 while (!test_stop) {
  ldthread_mutex_lock(&grp[gid].data_lock,235,__func__,"pthread.c");
  int i;
  for(i = 0; i < shared_data_size / sizeof(int); i++)
  {
   grp[gid].data[i]++;
   if(grp[gid].data[i] == 2e+31-1)
    grp[gid].data[i] = 0;
  }
  t->loop_count++;
  ldthread_mutex_unlock(&grp[gid].data_lock);
 }
}

main(int argc, char *argv[])
{

 ldthread_mutex_init(&lock,((void *)0),"&lock");
 ldthread_mutex_init(&atomic_lock,((void *)0),"&atomic_lock");

 int i, j;
 struct timeval btv, etv;
 double delta, d1, d2;

 read_options(argc, argv);

 grp = (struct group *)malloc(sizeof(struct group) * ngroups);
 if(!grp)
 {
  perror("malloc");
  exit(1);
 }

 for(i = 0; i < ngroups; i++)
 {
  grp[i].tids = (struct thread *)malloc(sizeof(struct thread)*group_size);
  if (!grp[i].tids)
  {
   perror("malloc");
   exit(1);
  }
  memset(grp[i].tids, 0, sizeof(struct thread)*group_size);
  ldthread_mutex_init(&grp[i].data_lock,((void *)0),"&grp[i].data_lock");
  grp[i].data = (unsigned int *)malloc(sizeof(unsigned int) * shared_data_size / sizeof(int));
  if(!grp[i].data)
  {
   perror("malloc");
   exit(1);
  }
 }

 ldthread_mutex_lock(&lock,285,__func__,"pthread.c");

 for (i=0; i < ngroups; i++)
 {
  for(j = 0; j < group_size; j++)
  {
   int *temp = (int *)malloc(sizeof(int));
   *temp = i;
   grp[i].tids[j].gid = i;
   ldthread_create(&grp[i].tids[j].tid,((void *)0),foo,&grp[i].tids[j],"&grp[i].tids[j].tid");
  }
 }

 while (atomic_read(&ready_count) != (ngroups * group_size))
  ;

 ldthread_mutex_unlock(&lock);
 usleep(duration);

 test_stop = 1;

 for (i=0; i < ngroups; i++)
  for(j = 0; j < group_size; j++)
   ldthread_join(grp[i].tids[j].tid, ((void *)0));


 long int grp_count, tot_count;
 tot_count = 0;
 for(i = 0; i < ngroups; i++)
 {
  printf("group %d\n", i);
  grp_count = 0;
  for(j = 0; j < group_size; j++)
  {
   grp_count += grp[i].tids[j].loop_count;
   printf("thread %d: %ld\n", j, grp[i].tids[j].loop_count);
  }
  printf("total: %ld\n", grp_count);
  tot_count += grp_count;
  printf("\n");
 }
 printf("Total count: %ld\n", tot_count);







}
