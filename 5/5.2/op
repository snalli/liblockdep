# 14 "pthread.c" 2
# 22 "pthread.c"
int ncpu;
int group_size;
int ngroups;
int shared_data_size;
int duration;
int test_stop = 0;
char mode;

ldthread_mutex_t lock;


struct thread {
 int gid;
 ldthread_t tid;
 long loop_count;
};

struct group {
 struct thread *tids;
 unsigned int *data;
 ldthread_mutex_t data_lock;
} *grp;

int maxcount = 100000;
int counter;

inline pid_t gettid()
{
 return syscall(224);
}

void usage(char *cmd)
{
 printf("Usage: %s [options]\n"
  "-c\t\tNo. of cpus (default: 2)\n"
  "-t\t\tSize of each thread group (default: 10)\n"
  "-g\t\tNo. of thread groups (default: 2)\n"
  "-d\t\tSize of shared data in bytes (default: 4)\n"
  "-u\t\tDuration of test run in us\n"
  "-m\t\tDuratino of test run in ms\n"
  "-s\t\tDuration	of test run in s\n"
  "-M\t\tMode of test run (default: normal)\n"
  "Note: If more than one option for test duration are specified then"
  "the value of last option will be considered\n", cmd);
}

void read_options(int argc, char *argv[]) {
 int option;

 ncpu = 2;
 group_size = 10;
 ngroups = 2;
 shared_data_size = 4;
 duration = 1e+3;

 while((option = getopt(argc, argv, "c:t:g:d:u:m:s:M:")) != -1)
 {
  switch(option)
  {
   case 'c':
    ncpu = atoi(optarg);
    break;
   case 't':
    group_size = atoi(optarg);
    break;
   case 'g':
    ngroups = atoi(optarg);
    break;
   case 'd':
    shared_data_size = atoi(optarg);
    break;
   case 'u':
    duration = atoi(optarg);
    break;
   case 'm':
    duration = atoi(optarg) * 1000;
    break;
   case 's':
    duration = atoi(optarg) * 1e+3;
    break;
   case 'M':
    mode = optarg[0];
    if(mode != 'b' && mode != 'n')
    {
     goto out;
    }
    break;
   default:
    goto out;
  }
 }
 return;
out:
 printf("Invalid option specified\n");
 usage(argv[0]);
 exit(1);
}

static inline void set_group(pid_t tid, char *group)
{
 int fd, rc;
 char buf[100];
 char pid[20];

 sprintf(buf, "/dev/cgroup/%s/tasks", group);
 fd = open(buf, 02);
 if (fd < 0)
  perror("open"), exit(1);

 sprintf(pid, "%d", tid);
 rc = write(fd, pid, strlen(pid));
 if (rc < 0)
  perror("write"), exit(1);

 close(fd);
}

ldthread_mutex_t atomic_lock;

int ready_count;

static inline void atomic_inc(int *x)
{
 ldthread_mutex_lock(&atomic_lock,145,__func__,"pthread.c");
 *x++;
 ldthread_mutex_unlock(&atomic_lock);
}

static inline int atomic_read(int *x)
{
 return *x;
}

